<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>FacebookAuthentication</servlet-name>
  	<servlet-class>com.cloudproject.main.MainServlet</servlet-class>
  </servlet>
  
  <servlet>
  	<servlet-name>Profile</servlet-name>
  	<servlet-class>com.cloudproject.main.ProfileServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>FacebookAuthentication</servlet-name>
  	<url-pattern>/fbhome</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  	<servlet-name>Profile</servlet-name>
  	<url-pattern>/profile</url-pattern>
  </servlet-mapping>
</web-app>
